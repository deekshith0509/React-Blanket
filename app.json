{
  "expo": {
    "name": "Sound Blanket",
    "slug": "sound-blanket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.deekshith.soundblanket",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-av",
      "expo-asset"
    ],
    "ios": {
      "bundleIdentifier": "com.deekshith.soundblanket"
    }
  }
}
